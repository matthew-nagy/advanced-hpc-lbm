#!/bin/bash

#SBATCH --job-name d2q9-bgk
#SBATCH --nodes 4
#SBATCH --ntasks-per-node 28
#SBATCH --exclude compute[101,107,117,300]
#SBATCH --time 00:20:00
#SBATCH --partition veryshort
#SBATCH --reservation=COSC026662
#SBATCH --account=COSC026662
#SBATCH --output d2q9-bgk.out

echo Running on host `hostname`
echo Time is `date`
echo Directory is `pwd`
echo Slurm job ID is $SLURM_JOB_ID
echo This job runs on the following machines:
echo `echo $SLURM_JOB_NODELIST | uniq`

mpirun -np 36 advixe-cl -v -collect survey -project-dir=./Intel/firstPeak -data-limit=0 ./d2q9-bgk input_1024x1024.params obstacles_1024x1024.dat
mpirun -np 36 advixe-cl -v -collect tripcounts --flop -project-dir=./Intel/firstPeak -data-limit=0 ./d2q9-bgk input_1024x1024.params obstacles_1024x1024.dat

mpirun -np 47 advixe-cl -v -collect survey -project-dir=./Intel/dip ./d2q9-bgk input_1024x1024.params obstacles_1024x1024.dat
mpirun -np 47 advixe-cl -v -collect tripcounts --flop -project-dir=./Intel/dip -data-limit=0 ./d2q9-bgk input_1024x1024.params obstacles_1024x1024.dat

mpirun -np 80 advixe-cl -v -collect survey -project-dir=./Intel/secondPeak ./d2q9-bgk input_1024x1024.params obstacles_1024x1024.dat
mpirun -np 80 advixe-cl -v -collect tripcounts --flop -project-dir=./Intel/secondPeak -data-limit=0 ./d2q9-bgk input_1024x1024.params obstacles_1024x1024.dat

mpirun -np 112 advixe-cl -v -collect survey -project-dir=./Intel/finale ./d2q9-bgk input_1024x1024.params obstacles_1024x1024.dat
mpirun -np 112 advixe-cl -v -collect tripcounts --flop -project-dir=./Intel/finale -data-limit=0 ./d2q9-bgk input_1024x1024.params obstacles_1024x1024.dat


mpirun -np 28 advixe-cl -v -collect survey -project-dir=./Intel/preDrop128 ./d2q9-bgk input_128x128.params obstacles_128x128.dat
mpirun -np 28 advixe-cl -v -collect tripcounts --flop -project-dir=./Intel/preDrop128 -data-limit=0 ./d2q9-bgk input_128x128.params obstacles_128x128.dat
mpirun -np 29 advixe-cl -v -collect survey -project-dir=./Intel/postDrop128 ./d2q9-bgk input_128x128.params obstacles_128x128.dat
mpirun -np 29 advixe-cl -v -collect tripcounts --flop -project-dir=./Intel/postDrop128 -data-limit=0 ./d2q9-bgk input_128x128.params obstacles_128x128.dat

mpirun -np 28 advixe-cl -v -collect survey -project-dir=./Intel/preDrop256 ./d2q9-bgk input_256x256.params obstacles_256x256.dat
mpirun -np 28 advixe-cl -v -collect tripcounts --flop -project-dir=./Intel/preDrop256 -data-limit=0 ./d2q9-bgk input_256x256.params obstacles_256x256.dat
mpirun -np 29 advixe-cl -v -collect survey -project-dir=./Intel/postDrop256 ./d2q9-bgk input_256x256.params obstacles_256x256.dat
mpirun -np 29 advixe-cl -v -collect tripcounts --flop -project-dir=./Intel/postDrop256 -data-limit=0 ./d2q9-bgk input_256x256.params obstacles_256x256.dat


